[flake8]
# Black compatibility
# https://github.com/psf/black/blob/06ccb88bf2bd35a4dc5d591bb296b5b299d07323/docs/guides/using_black_with_other_tools.md#flake8
max-line-length = 88
extend-ignore =
    E203,
    
    ; Other ignores

    ; Ignore line length compleatly
    E501,
    ; Ignore assigning lambda expressions
    E731
